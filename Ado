Template for java application



trigger: none
pr: none
# pool: 'CloudOps-AP-SoutheastAsia-Linux'
pool:
 vmImage: 'ubuntu-latest'
resources:
 repositories:
   - repository: elc-sre
     type: github
     name: elc-digital/sre-azdo-templates
     endpoint: elc-digital
variables:
 - template: templates/vars.yml@elc-sre
steps:
 - checkout: self
 - script: mvn clean package
   
displayName: Mvn Build
   workingDirectory: '$(Build.SourcesDirectory)'
 - task: PublishBuildArtifacts@1
   inputs:
     PathtoPublish: '$(Build.SourcesDirectory)'
     ArtifactName: 'digital artifact'
     # publishLocation: 'Container'
 
#Scan the JAR file using ShiftLeft
 - template: templates/executeShiftLeft-v2.yml@elc-sre
   parameters:
     SHIFTLEFT_APP: "elccep-DigitalCapture-backend"
     SHIFTLEFT_BRANCH: 'dev'
     SHIFTLEFT_WORKING_DIR: '.'
     SHIFTLEFT_ARG: '--java dgc-web/target/dgc-web-1.0.0.jar'
has context menu

------------------------------------------------------------------------------
     
trigger: none
pr: none
pool:
  vmImage: 'ubuntu-latest'
  
strategy:
  matrix:
    Python38:
      python.version: '3.8'
      
resources:
  repositories:
    - repository: elc-sre
      type: github
      name: elc-digital/sre-azdo-templates
      endpoint: elc-digital
variables:
  - template: templates/vars.yml@elc-sre
steps:
- task: UsePythonVersion@0
  inputs:
    versionSpec: '$(python.version)'
  displayName: 'Use Python $(python.version)'
- template: templates/executeShiftLeft-v1.yml@elc-sre
  parameters:
    SHIFTLEFT_APP: "Green Score Management"
    SHIFTLEFT_BRANCH: 'condor-qa-azfunc'
    SHIFTLEFT_WORKING_DIR: '.'
    SHIFTLEFT_ARG: '--python --tag branch=condor-qa-azfunc'
