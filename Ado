Template for non-java application (if repo is in git hub)


trigger: none
pr: none
pool:
 vmImage: 'ubuntu-latest'
    
resources:
 repositories:
   - repository: elc-sre
     type: github
     name: elc-digital/sre-azdo-templates
     endpoint: github.com
 
variables:
 - template: templates/vars.yml@elc-sre
 
steps:
 
- template: templates/executeShiftLeft-v2.yml@elc-sre
 parameters:
   SHIFTLEFT_APP: "refunds-service"
   SHIFTLEFT_BRANCH: 'main'
   SHIFTLEFT_WORKING_DIR: '.'
   SHIFTLEFT_ARG: '--js --tag branch=main'  

